{
  "name": "jungle-lab-studio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "electron.cjs",
  "scripts": {
    "dev": "tauri dev",
    "build": "vite build && tauri build",
    "tauri": "tauri",
    "electron": "vite build && electron ./electron.cjs",
    "electron:package": "vite build && electron-builder",
    "test": "vitest",
    "lint": "eslint src/App.tsx src/main.tsx \"src/theme/**/*.{ts,tsx}\" \"src/utils/a11y/**/*.{ts,tsx}\" --max-warnings=0",
    "jarvis:dev": "python jarvis_core/JarvisCore.py --models-dir ./models"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@tauri-apps/api": "^1.5.0",
    "@types/three": "^0.179.0",
    "@ant-design/icons": "^5.3.7",
    "@ant-design/pro-components": "^2.7.13",
    "antd": "^5.21.0",
    "framer-motion": "^11.11.11",
    "highlight.js": "^11.9.0",
    "monaco-editor": "^0.52.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-window": "^1.8.10",
    "simple-git": "^3.27.0",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-window": "^1.8.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.2.0",
    "electron": "^37.3.1",
    "electron-builder": "^24.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.2.0",
    "vite": "^7.1.3",
    "vitest": "^1.5.0"
  },
  "build": {
    "appId": "com.junglelabstudio.app",
    "files": [
      "dist/**/*",
      "electron.cjs",
      "plugin-manager.cjs"
    ],
    "extraResources": [
      {
        "from": "jarvis_core",
        "to": "jarvis_core",
        "filter": [
          "**/*"
        ]
      }
    ],
    "extraMetadata": {
      "main": "electron.cjs"
    }
  }
}
