{
  "name": "Gen Lab",
  "description": "Advanced generative laboratory with atmospheric effects",
  "author": "AudioVisualizer",
  "version": "1.0.0",
  "category": "generative",
  "tags": ["cloud", "vapor", "skybox", "noise", "audio-reactive"],
  "thumbnail": "gen_lab_thumb.png",
